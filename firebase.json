{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "redirects":[ 
    {
      "source": "/",
      "destination": "getStarted/landing.html",
      "type": 301
    }
  ]
  ,
  "storage": {
    "rules": "storage.rules"
  }
}
